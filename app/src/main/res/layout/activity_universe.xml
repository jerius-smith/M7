<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".views.UniverseActivity"
    android:background="@drawable/star032">

    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:columnCount="6"
        android:paddingTop="75dp"
        android:paddingBottom="75dp"
        android:rowCount="6"
        android:stretchColumns="0,1"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">

        <View
            android:id="@+id/emptyview11"
            android:layout_width="58dp"
            android:layout_height="64dp"
            android:layout_row="0"
            android:layout_rowSpan="1"
            android:layout_column="0"
            android:layout_columnSpan="1" />

        <View
            android:id="@+id/emptyview12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="1"
            android:layout_columnSpan="1"
            android:layout_rowSpan="1"/>

        <View
            android:id="@+id/emptyview13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="2"
            android:layout_columnSpan="1"
            android:layout_rowSpan="1"/>

        <View
            android:id="@+id/emptyview14"
            android:layout_width="110dp"
            android:layout_height="101dp"
            android:layout_row="0"
            android:layout_column="3"
            android:layout_columnSpan="1"
            android:layout_rowSpan="1"/>

        <View
            android:id="@+id/emptyview15"
            android:layout_width="110dp"
            android:layout_height="101dp"
            android:layout_row="0"
            android:layout_column="4"
            android:layout_columnSpan="1"
            android:layout_rowSpan="1"/>

        <View
            android:id="@+id/emptyview16"
            android:layout_width="110dp"
            android:layout_height="101dp"
            android:layout_row="0"
            android:layout_column="5"
            android:layout_columnSpan="1"
            android:layout_rowSpan="1"/>


        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="110dp"
            android:layout_height="101dp"
            android:layout_row="2"
            android:layout_column="2"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/solarsystem2" />

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="110dp"
            android:layout_height="101dp"
            android:layout_row="3"
            android:layout_column="3"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/solarsystem3" />

        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="110dp"
            android:layout_height="101dp"
            android:layout_row="4"
            android:layout_column="4"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/solarsystem4" />
    </TableLayout>
</RelativeLayout>